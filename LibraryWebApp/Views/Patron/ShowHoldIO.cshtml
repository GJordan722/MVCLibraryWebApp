@model SearchModel
@using System.Text;
@using Microsoft.AspNetCore.Mvc;

@{
    ViewData["Title"] = "View HoldIO - Patron";

}

@if (Model.holdiolist.Count > 0)
{
    <div class="row">
        <div class="col-sm-5">
        </div>
        <div class="col-sm-5"></div>
        <div class="col-sm-2">
            @Html.ActionLink("Back","Dashboard","Home", null,new{@class="btn btn-success"})
        </div>
    </div>
    <div class="d-inline-block">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Media ID</th>
                    <th scope="col">Title</th>
                </tr>
            </thead>
            <tbody>
                @foreach (HoldIOModel mm in Model.holdiolist)
                {
                    <tr scope="row">
                        <td>@mm.MediaID</td>
                        <td>@mm.MediaName</td
                    </tr>
                }

            </tbody>
        </table>
    </div>
}
else
{
    <p>No Results found</p>
    @Html.ActionLink("Back","Dashboard","Home", null,new{@class="btn btn-success"})
}